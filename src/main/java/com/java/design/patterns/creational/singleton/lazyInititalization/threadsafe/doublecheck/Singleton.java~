package com.java.design.patterns.creational.singleton.lazyInititalization.threadsafe.doublecheck;

public class Singleton {
    // Lazy Initialization
    private static volatile Singleton instance;

    private Singleton() {}

    public static Singleton getInstance() {

        if (instance == null) {
            synchronized (Singleton.class){
                if(instance == null){
                    instance = new Singleton();
                }
            }
        }
       return instance;
    }
}
